<!DOCTYPE html>
<html>
<head>
  <title>Lms</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <link href='//fonts.googleapis.com/css?family=News+Cycle:400,700' rel='stylesheet' type='text/css'>

  <% if Rails.env.production? %>
    <script src='//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js'></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/react/0.12.1/react.min.js"></script>
  <% else %>
    <%= javascript_include_tag '_dev/jquery.1.11.1.js' %>
    <%= javascript_include_tag '_dev/react.0.12.0.js' %>
  <% end %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="panel">
    <div class="course-name">
      <a href="/">FEWD 24</a>
    </div>
    <div class="inner">
    </div>
  <% if current_user %>
    <div class="user">
      <div><%= current_user.name %></div>
    </div>
    <% end %>
  </div><div id="main">
    <%= yield %>
  </div>
</body>
</html>
