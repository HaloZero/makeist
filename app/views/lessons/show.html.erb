<div class="heading">
  <h2>Lesson <%= @lesson.lesson.to_i %> &rarr; <%= @lesson.date_human %></h2>
</div>
<div id="NavTabs"></div>
<script>
  window.MK = {
    LESSON_ID : '<%= @lesson.id.to_s %>',
    USER : <%= current_user.to_api.to_json.html_safe %>,
    CSRFTOKEN : '<%= form_authenticity_token %>'
  };

  MK.Nav = React.render(NavTabs({
          data: [
              { name: 'Materials', content : null }
              ,
              {
                  name: 'My Feedback',
                  content : Feedback,
                  async : function() {
                      return $.ajax({
                          url: '/students/' + MK.USER.id + '/lessons/' + MK.LESSON_ID + '/feedback',
                          dataType: "JSON"
                      })
                  }
              }
          ]
      }), document.getElementById('NavTabs'));
</script>
